{
    "$schema": "https://schemas.wp.org/wp/6.9/theme.json",
    "settings": {
        "appearanceTools": true,
        "color": {
            "gradients": [],
            "palette": [
                {
                    "color": "var(--nv-site-bg, #FFFFFF)",
                    "name": "Background",
                    "slug": "ti-bg"
                },
                {
                    "color": "var(--nv-text-color, #202020)",
                    "name": "Foreground",
                    "slug": "ti-fg"
                },
                {
                    "color": "#397999",
                    "name": "Accent",
                    "slug": "ti-accent"
                },
                {
                    "color": "#5cd4e0",
                    "name": "Accent Secondary",
                    "slug": "ti-accent-secondary"
                },
                {
                    "color": "#0a0a0a",
                    "name": "Background Dark",
                    "slug": "ti-bg-inv"
                },
                {
                    "color": "var(--nv-light-bg, #f7f7f3)",
                    "name": "Background Alt",
                    "slug": "ti-bg-alt"
                },
                {
                    "color": "var(--nv-text-dark-bg, #FBFBFB)",
                    "name": "Foreground Alt",
                    "slug": "ti-fg-alt"
                }
            ]
        },
        "custom": {
            "spacing": {
                "baseline": "16px",
                "gap": {
                    "horizontal": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 2.222vw, calc( 2 * var( --wp--custom--spacing--baseline ) ) )",
                    "vertical": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )"
                },
                "gutter": "clamp( calc( 1.5 * var( --wp--custom--spacing--baseline ) ), 3.333vw, calc( 3 * var( --wp--custom--spacing--baseline ) ) )",
                "outer": "var( --wp--custom--spacing--gutter )",
                "small": "min(24px, 6.4vw)"
            },
            "typography": {
                "adaptiveRatio": "1.5",
                "fluidSize": "calc(var(--wp--custom--typography--root-font-size) * var(--wp--custom--typography--adaptive-ratio) * (100vw/1280))",
                "fontSmoothing": {
                    "moz": "grayscale",
                    "webkit": "antialiased"
                },
                "largeMax": "28px",
                "largeMin": "24px",
                "mediumMax": "24px",
                "mediumMin": "20px",
                "normalMax": "18px",
                "normalMin": "16px",
                "rootFontSize": "12",
                "scale": "1.333",
                "smallMax": "14px",
                "smallMin": "12px",
                "xLargeMax": "40px",
                "xLargeMin": "28px",
                "xxLargeMax": "56px",
                "xxLargeMin": "36px"
            }
        },
        "layout": {
            "contentSize": "740px",
            "wideSize": "1140px"
        },
        "spacing": {
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ]
        },
        "typography": {
            "customFontSize": true,
            "defaultFontSizes": false,
            "fluid": true,
            "fontFamilies": [
                {
                    "fontFace": [
                        {
                            "fontFamily": "\"Open Sans\"",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "300 900",
                            "src": [
                              "file:./fonts/OpenSans-VariableFont.ttf"
                            ]
                        }
                    ],
                    "fontFamily": "\"Open Sans\", sans-serif",
                    "name": "Open Sans",
                    "slug": "open-sans"
                }
            ],
            "fontSizes": [
                {
                    "name": "Small",
                    "size": "clamp(var(--wp--custom--typography--small-min), calc(var(--wp--custom--typography--fluid-size) / var(--wp--custom--typography--scale)), var(--wp--custom--typography--small-max))",
                    "slug": "small"
                },
                {
                    "name": "Normal",
                    "size": "clamp(var(--wp--custom--typography--normal-min), var(--wp--custom--typography--fluid-size), var(--wp--custom--typography--normal-max))",
                    "slug": "normal"
                },
                {
                    "name": "Medium",
                    "size": "clamp(var(--wp--custom--typography--medium-min), calc(var(--wp--custom--typography--fluid-size) * var(--wp--custom--typography--scale) * 1), var(--wp--custom--typography--medium-max))",
                    "slug": "medium"
                },
                {
                    "name": "Large",
                    "size": "clamp(var(--wp--custom--typography--large-min), calc(var(--wp--custom--typography--fluid-size) * var(--wp--custom--typography--scale) * 1.5), var(--wp--custom--typography--large-max))",
                    "slug": "large"
                },
                {
                    "name": "Extra Large",
                    "size": "clamp(var(--wp--custom--typography--x-large-min), calc(var(--wp--custom--typography--fluid-size) * var(--wp--custom--typography--scale) * 2), var(--wp--custom--typography--x-large-max))",
                    "slug": "x-large"
                },
                {
                    "name": "Huge",
                    "size": "clamp(var(--wp--custom--typography--xx-large-min), calc(var(--wp--custom--typography--fluid-size) * var(--wp--custom--typography--scale) * 3), var(--wp--custom--typography--xx-large-max))",
                    "slug": "huge"
                }
            ]
        }
    },
    "styles": {
        "blocks": {
            "core/button": {
                "border": {
                    "bottom": {
                        "style": "none",
                        "width": "0px"
                    },
                    "color": "var(--wp--preset--color--ti-accent)",
                    "left": {
                        "style": "none",
                        "width": "0px"
                    },
                    "radius": "var(--neve-button-border-radius, 5px)",
                    "right": {
                        "style": "none",
                        "width": "0px"
                    },
                    "style": "solid",
                    "top": {
                        "style": "none",
                        "width": "0px"
                    },
                    "width": "2px"
                },
                "color": {
                    "background": "var(--wp--preset--color--ti-accent)",
                    "text": "var(--wp--preset--color--ti-fg-alt)"
                },
                "spacing": {
                    "padding": {
                        "bottom": "16px",
                        "left": "40px",
                        "right": "40px",
                        "top": "16px"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--normal)",
                    "fontStyle": "normal",
                    "fontWeight": "600"
                }
            },
            "core/comment-author-name": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--normal)"
                }
            },
            "core/comment-content": {
                "border": {
                    "radius": "5px"
                },
                "color": {
                    "background": "var(--wp--preset--color--ti-bg)"
                },
                "spacing": {
                    "padding": {
                        "bottom": "16px",
                        "left": "16px",
                        "right": "16px",
                        "top": "16px"
                    }
                }
            },
            "core/comment-date": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "fontSize": "14px",
                    "textTransform": "uppercase"
                }
            },
            "core/comment-edit-link": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "fontSize": "14px",
                    "textTransform": "uppercase"
                }
            },
            "core/comment-reply-link": {
                "elements": {
                    "link": {
                        "color": {
                            "text": "var(--wp--preset--color--ti-accent)"
                        },
                        "typography": {
                            "fontSize": "var(--wp--preset--font-size--small)"
                        }
                    }
                },
                "typography": {
                    "textDecoration": "none",
                    "textTransform": "uppercase"
                }
            },
            "core/latest-posts": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "600"
                }
            },
            "core/navigation": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/post-excerpt": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/post-terms": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/post-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                }
            },
            "core/quote": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)"
                }
            },
            "core/search": {
                "border": {
                    "radius": "5px"
                }
            },
            "core/site-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "600"
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--ti-bg)",
            "text": "var(--wp--preset--color--ti-fg)"
        },
        "elements": {
            "button": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--open-sans)"
                }
            },
            "caption": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--open-sans)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--neve-font-size-h1, var(--wp--preset--font-size--huge))",
                    "fontWeight": "var(--neve-font-weight-h1, 600)",
                    "lineHeight": "var(--neve-font-line-height-h1, 1.25)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--neve-font-size-h2, var(--wp--preset--font-size--x-large))",
                    "fontWeight": "var(--neve-font-weight-h2, 600)",
                    "lineHeight": "var(--neve-font-line-height-h2, 1.35)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--neve-font-size-h3, var(--wp--preset--font-size--large))",
                    "fontWeight": "var(--neve-font-weight-h3, 600)",
                    "lineHeight": "var(--neve-font-line-height-h3, 1.35)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--neve-font-size-h4, var(--wp--preset--font-size--medium))",
                    "fontWeight": "var(--neve-font-weight-h4, 600)",
                    "lineHeight": "var(--neve-font-line-height-h4, 1.4)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--neve-font-size-h5, var(--wp--preset--font-size--normal))",
                    "fontWeight": "var(--neve-font-weight-h5, 600)",
                    "lineHeight": "var(--neve-font-line-height-h5, 1.55)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--neve-font-size-h6, var(--wp--preset--font-size--small))",
                    "fontWeight": "var(--neve-font-weight-h6, 600)",
                    "lineHeight": "var(--neve-font-line-height-h6, 1.6)"
                }
            },
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--open-sans)",
                    "fontSize": "var(--neve-font-size-heading, var(--wp--preset--font-size--huge))",
                    "fontStyle": "normal",
                    "fontWeight": "300",
                    "lineHeight": "var(--neve-font-line-height-heading, 1.3)"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--ti-fg)"
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--open-sans)"
                }
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--open-sans)",
            "fontSize": "var(--neve-font-size-body, var(--wp--preset--font-size--normal))",
            "fontStyle": "normal",
            "fontWeight": "300",
            "lineHeight": "var(--neve-font-line-height-body, 1.7)"
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header_default",
            "title": "Header Variation - Default"
        },
        {
            "area": "header",
            "name": "header_frontpage",
            "title": "Header Variation - Homepage"
        },
        {
            "area": "footer",
            "name": "footer_default",
            "title": "Footer Variation - Default"
        }
    ],
    "version": 3
}
